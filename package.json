{
    "name": "rsfjs",
    "version": "1.0.0",
    "description": "A lightweight JavaScript framework for building reactive web applications",
    "main": "rsf.js",
    "module": "rsf.js",
    "browser": "rsf.js",
    "files": [
        "rsf.js",
        "rsf.min.js",
        "rsf-static.js",
        "rsf-static.min.js",
        "renderHtmlToRsf.js",
        "renderHtmlToRsf.min.js",
        "renderReactive.js",
        "renderReactive.min.js",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "test": "echo \"Open tests/index.html in a browser to run tests\" && exit 0",
        "minify": "npm run minify:rsf && npm run minify:static && npm run minify:html && npm run minify:reactive",
        "minify:rsf": "terser rsf.js -c -m -o rsf.min.js",
        "minify:static": "terser rsf-static.js -c -m -o rsf-static.min.js",
        "minify:html": "terser renderHtmlToRsf.js -c -m -o renderHtmlToRsf.min.js",
        "minify:reactive": "terser renderReactive.js -c -m -o renderReactive.min.js",
        "build": "npm run minify"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rsoppelsa/rsfjs.git"
    },
    "keywords": [
        "framework",
        "reactive",
        "javascript",
        "ui",
        "lightweight",
        "zero-dependencies",
        "dom",
        "state-management",
        "ui-framework",
        "no-build"
    ],
    "author": "rsoppelsa",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rsoppelsa/rsfjs/issues"
    },
    "homepage": "https://github.com/rsoppelsa/rsfjs#readme",
    "engines": {
        "node": ">=10.0.0"
    },
    "devDependencies": {
        "terser": "^5.44.1"
    }
}
